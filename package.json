{
    "name": "alhadi-fullstack",
    "private": true,
    "version": "1.0.0",
    "scripts": {
      "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
      "dev:frontend": "npm --prefix frontend run dev",
      "dev:backend": "npm --prefix api run dev",
      "build": "npm run build:frontend && npm run build:backend",
      "build:frontend": "npm --prefix frontend run build",
      "build:backend": "npm --prefix api run build",
      "start": "npm run build && npm run start:frontend && npm run start:backend",
      "start:frontend": "npm --prefix frontend run preview",
      "start:backend": "npm --prefix api run start"
    },
    "dependencies": {
      "concurrently": "^7.3.0"
    },
    "devDependencies": {}
  }